        "@supabase/supabase-js": "^2.8.0",
        "isbot": "^3.6.6",
        "pino": "^8.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.20.6"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.13.0",
        "@remix-run/eslint-config": "^1.13.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "eslint": "^8.34.0",
        "prisma": "^4.10.1",
        "tailwindcss": "^3.2.7",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=14"
